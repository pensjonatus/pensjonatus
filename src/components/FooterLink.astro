---
interface Props {
  href: string;
  label: string;
}
const { href, label } = Astro.props;
---

<a href={href} class="footer-link" target="_blank" rel="noopener noreferrer"
  >{label}</a
>

<style>
  a.footer-link {
    color: white;
    text-decoration: none;
  }

  a.footer-link:visited {
    color: lightblue;
  }
</style>
